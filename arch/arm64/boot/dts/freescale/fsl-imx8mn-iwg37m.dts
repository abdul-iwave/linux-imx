				MX8MN_IOMUXC_SAI5_RXFS_GPIO3_IO19		0x41
				MX8MN_IOMUXC_SAI5_RXC_GPIO3_IO20		0x41
			>;
		};

		pinctrl_csi_rst: csi_rst_grp { /* MIPI CSI: OV5640 Camera IOMUX Pin Configuration */
